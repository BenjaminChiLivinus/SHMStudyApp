<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.shm.shmstudy">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".home.HomeActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".study.StudiesActivity" android:label="@string/studies" android:parentActivityName=".home.HomeActivity"/>
        <activity android:name=".Login" android:label="@string/login"/>
        <activity android:name=".Settings" android:label="@string/settings" android:parentActivityName=".home.HomeActivity"/>
        <activity android:name=".inspiration.InspirationActivity" android:label="@string/inspiration" android:parentActivityName=".home.HomeActivity"/>
        <activity android:name=".library.LibraryActivity" android:label="@string/library" android:parentActivityName=".home.HomeActivity"/>
        <activity android:name=".Notes" android:label="@string/notes" android:parentActivityName=".home.HomeActivity"/>
        <activity android:name=".Comments" android:label="@string/comments" android:parentActivityName=".home.HomeActivity"/>
        <activity android:name=".About" android:label="@string/about" android:parentActivityName=".home.HomeActivity"/>
        <activity android:name=".Help" android:label="@string/help" android:parentActivityName=".home.HomeActivity"/>
        <activity android:name=".me.MeActivity" android:label="@string/me" android:parentActivityName=".home.HomeActivity"/>
        <activity android:name=".IntroSlideOne"/>
        <activity android:name=".IntroSlideTwo"/>
        <activity android:name=".IntroSlideThree"/>
    </application>

</manifest>